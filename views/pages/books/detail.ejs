<section class="card">
    <h2>
        <%= books.title %>
    </h2>
    <img src='<%=books.image_url %>' alt="books Cover">
    <h3>
        <%= books.author %>
    </h3>
    <p class="description">
        <%= books.description %>
    </p>
    <p>
        ISBN: <%= books.isbn %>
    </p>
    <p>
        booksshelf: <%= books.offShelf %>
    </p>
</section